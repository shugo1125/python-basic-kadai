      "source": [
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "for i in range(0, 9):\n",
        "    # ループ変数iの値が奇数（2で割った余りが1）であれば、値を出力せずにcontinue文で次のループに進む\n",
        "    print(array[i])\n",
        "num=0\n",
        "while num != 9:\n",
        "    # 次の条件式で比較される、変数numの現在の値を出力する\n",
        "    print(array[num])\n",
        "    num += 1"
      ]
